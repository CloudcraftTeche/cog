{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["unused-imports"],
  "rules": {
    "no-console": "error",
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": "warn"
  }
}
